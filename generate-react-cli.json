{
	"usesTypeScript": true,
	"usesCssModule": true,
	"cssPreprocessor": "css",
	"testLibrary": "None",
	"component": {
		"default": {
			"customTemplates": {
				"component": "templates/default/TemplateName.tsx",
				"style": "templates/default/TemplateName.module.css"
			},
			"path": "src/components",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false
		},
		"page": {
			"path": "src/pages",
			"withLazy": true,
			"withStory": false,
			"withStyle": true,
			"withTest": true
		}
	}
}
